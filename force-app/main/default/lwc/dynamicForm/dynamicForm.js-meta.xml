<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>56.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Dynamic Form</masterLabel>
    <description>Component that allows users to dynamically create froms for objects: Track, Tag, Ping</description>
    <targets>
        <target>lightning__RecordPage</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__RecordPage">
            <property label="Enter the object you want to create" name="baseObject" type="String" datasource="Track,Tag,Ping" default="Track"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>